{
  "name": "easyauth-server",
  "version": "1.0.0",
  "description": "Auth servr",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "bun --watch src/index.js",
    "build": "prisma generate",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest tests/integration --run",
    "test:performance": "node tests/performance/rate_limit_benchmark.js",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit && npm outdated",
    "security:owasp": "retire --path src/",
    "benchmark:all": "npm run test:performance",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "bun run prisma/seed.js",
    "db:cleanup": "node src/utils/cleanup.js",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@vitest/ui": "^3.2.4",
    "autocannon": "^7.15.0",
    "bun-types": "^1.2.18",
    "eslint": "^9.31.0",
    "prettier": "^3.6.2",
    "prisma": "^5.22.0",
    "retire": "^4.0.3",
    "supertest": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "authentication",
    "jwt",
    "auth",
    "api",
    "security",
    "bun"
  ],
  "author": "Mythsoul / moon ",
  "license": "MIT"
}
